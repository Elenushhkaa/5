#include<iostream>
using namespace std;


int main() {
    srand(time(0));
    int Elenushka, X, n, Bylat;
    cin >> Elenushka >> Bylat >> X;
    int** mas = new int* [Elenushka];
    for (int i = 0; i < Elenushka; i++) {
        mas[i] = new int[Bylat];

    }
    for (int i = 0; i < Elenushka; i++) {
        for (int j = 0; j < Bylat; j++) {
            mas[i][j] = rand() % 10;
        }
    }
    for (int i = 0; i < Elenushka; i++) {
        for (int j = 0; j < Bylat; j++) {
            cout << mas[i][j] << " ";
        }
        cout << endl;
    }
    n = 0;
    for (int i = 0; i < Elenushka; i++) {
        for (int j = 0; j < Bylat; j++) {
            if (mas[i][j] == X) {

                n++;
            }
        }
    }
    cout << n;


}
